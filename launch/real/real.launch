<launch>
    <arg name="tg" default="dispersion" />

    <include file="$(find mushr_rhc)/launch/mapserver.launch" />

    <node pkg="mushr_rhc" type="rhcnode.py" name="rhcontroller" output="screen">
        <env name="RHC_USE_CUDA" value="0" />

		<param name="inferred_pose_t" value="/pf/inferred_pose" />

        <rosparam file="$(find mushr_rhc)/launch/params/trajgen/$(arg tg).yaml" />
        <rosparam file="$(find mushr_rhc)/launch/params/all_params.yaml" />
        <rosparam file="$(find mushr_rhc)/launch/real/params.yaml" />
    </node>
</launch>
